{
  "extends": [
    "eslint: recommended",
    "plugin:import/errors",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": ["react", "import", "jsx-a11y"],
  "rules": {
    "react/prop-types": 0,
    "no-console": 0
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
